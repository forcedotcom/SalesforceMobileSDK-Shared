{
    "name": "salesforcemobilesdk-shared",
    "version": "13.0.2",
    "dependencies": {
        "jquery": "2.0.3",
        "underscore": "1.5.2",
        "backbone": "1.1.0",
        "qunit": "v1.2.0",
        "ratchet": "1.0.2",
        "ratchet2": "twbs/ratchet#v2.0.2",
        "fastclick": "0.6.2",
        "react":"v0.12.0",
        "promise-polyfill":"6.0.2"
    },
    "exportsOverride": {
        "jquery": {
            ".": [
                "jquery.js",
                "jquery.min.js"
            ]
        },
        "backbone": {
            ".": "backbone*.js"
        },
        "underscore": {
            ".": "underscore*.js"
        },
        "qunit": {
            ".": [ 
                "qunit/qunit.js",
                "qunit/qunit.css"
            ]
        },
        "ratchet": {
            ".": [
                "dist/*.css"
            ]
        },
        "ratchet2": {
            ".": [
                "dist/css/*.css"
            ]
        },
        "fastclick": {
            ".": "lib/fastclick.js"
        },
        "react": {
            ".": "*.js"
        }
    }
}
